{
  "name": "generator-express-ts-antistress",
  "version": "1.1.3",
  "description": "A Yeoman generator to scaffold an Express.js TypeScript project using MVC and Layered Architecture.",
  "main": "./dist/generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "generator",
    "express",
    "typescript",
    "antistress",
    "oop",
    "mvc",
    "layered",
    "rest",
    "api",
    "boilerplate"
