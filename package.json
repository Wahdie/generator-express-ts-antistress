    "ejs": "^3.1.10",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@yeoman/types": "^1.6.0",
    "ajv": "^8.17.1",
    "chalk": "^5.4.1",
